"                                </t>\n"
"                                <t t-else=\"\">username</t>\n"
"                                <t t-esc=\"base_url\"></t>\n"
"                            </td>\n"
"                        </tr>\n"
"                        <tr>\n"
"                            <th>Team name</th>\n"
"                            <td>\n"
"                                <t t-if=\"object.team_id\">\n"
"                                    <t t-esc=\"object.team_id.name\"></t>\n"
